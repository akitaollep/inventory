<html>
	<head>
		<style type="text/css">
			body {
				font-family: monospace;
				font-size: 12px;
			}
		</style>
	</head>
	<body>
		<table border="0" style="width: 100%; margin-top: 130px;">
			<tr>
				<td style="width: 10%;"></td>
				<td style="width: 10%;"></td>
				<td style="width: 10%;"></td>
				<td style="width: 10%;"></td>
				<td style="width: 10%;"></td>
				<td style="width: 10%;"></td>
				<td colspan="3" style="width: 30%;"><%= receipt.receiptDate %></td>
				<td style="width: 10%;"></td>
			</tr>
		</table>

		<table border="0" style="width: 100%; margin-top: 35px;">
			<tr>
				<td style="width: 10%;"></td>
				<td colspan="2" style="width: 20%;"><%= receipt.order.respondentFirstName %> <%= receipt.order.respondentLastName %></td>
				<td style="width: 10%;"></td>
				<td style="width: 10%;"></td>
				<td style="width: 10%;"></td>
				<td colspan="3" style="width: 30%;"><%= receipt.order.address %></td>
				<td style="width: 10%;"></td>
			</tr>
		</table>

		<table border="0" style="width: 100%; margin-top: 15px;">
			<tr>
				<td style="width: 10%;"></td>
				<td colspan="2" style="width: 20%;"><%= receipt.amount %></td>
				<td style="width: 10%;"></td>
				<td style="width: 10%;"></td>
				<td style="width: 10%;"></td>
				<td style="width: 10%;"></td>
				<td style="width: 10%;"></td>
				<td style="width: 10%;"></td>
				<td style="width: 10%;"></td>
			</tr>
		</table>

		<table border="0" style="width: 100%; margin-top: 40px;">
			<tr>
				<td style="width: 10%;"></td>
				<td style="width: 10%;"></td>
				<td colspan="6" style="width: 40%;"><%= receipt.order.type %></td>
				<td colspan="2" style="width: 40%;"><%= receipt.amount %></td>
			</tr>
		</table>
		<table border="0" style="width: 100%; margin-top: 50px;">
			<tr>
				<td style="width: 10%;"></td>
				<td style="width: 10%;"></td>
				<td colspan="6" style="width: 40%;"><%= receipt.notes %></td>
				<td colspan="2" style="width: 40%;"></td>
			</tr>
		</table>
	</body>
</html>
